{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ps-ade"
		},
		"ADLS_LS_RAW_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS_RAW'"
		},
		"ADLS_LS_STAGE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS_STAGE'"
		},
		"sql02stgdb_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql02stgdb_LS'"
		},
		"ADLS_LS_RAW_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspsade.dfs.core.windows.net/"
		},
		"ADLS_LS_STAGE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspsade.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ps-ade_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS_RAW_accountKey": {
						"value": "[parameters('ADLS_LS_RAW_accountKey')]"
					},
					"ADLS_LS_STAGE_accountKey": {
						"value": "[parameters('ADLS_LS_STAGE_accountKey')]"
					},
					"sql02stgdb_LS_connectionString": {
						"value": "[parameters('sql02stgdb_LS_connectionString')]"
					},
					"ADLS_LS_RAW_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_RAW_properties_typeProperties_url')]"
					},
					"ADLS_LS_STAGE_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_STAGE_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}